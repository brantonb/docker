{
  "version": "2",
  "templates": [
    {
      "type": 3,
      "title": "Duck DNS",
      "description": "Free dynamic DNS hosted on AWS",
      "categories": [
        "networking"
      ],
      "platform": "linux",
      "repository": {
        "url": "https://github.com/brantonb/docker",
        "stackfile": "duckdns/docker-compose.yml"
      },
      "env": [
        {
          "name": "SUBDOMAINS",
          "label": "Subdomains",
          "description": "Comma-separated list of subdomains to update."
        },
        {
          "name": "TOKEN",
          "label": "Token",
          "description": "Obtained from account page on duckdns.org."
        },
        {
          "name": "TZ",
          "label": "Timezone",
          "default": "America/Los_Angeles"
        }
      ]
    },
    {
      "type": 3,
      "title": "Gollum",
      "description": "A git-based Wiki",
      "categories": [
        "wiki"
      ],
      "platform": "linux",
      "repository": {
        "url": "https://github.com/brantonb/docker",
        "stackfile": "gollum/docker-compose.yml"
      },
      "env": [
        {
          "name": "VIRTUAL_HOST",
          "label": "Virtual Host",
          "description": "The host name used by Nginx Proxy."
        }
      ]
    },
    {
      "type": 3,
      "title": "Nginx Proxy",
      "description": "Automated nginx proxy for Docker containers using docker-gen",
      "categories": [
        "networking"
      ],
      "platform": "linux",
      "repository": {
        "url": "https://github.com/brantonb/docker",
        "stackfile": "nginx-proxy/docker-compose.yml"
      },
      "env": [
          {
              "name": "VHOST_CONFIG_DIR",
              "label": "Virtual hosts configuration directory"
          }
      ]
    },
    {
      "type": 3,
      "title": "Pi-hole",
      "description": "Network-wide Ad blocking",
      "categories": [
        "ad blocking",
        "networking"
      ],
      "platform": "linux",
      "repository": {
        "url": "https://github.com/brantonb/docker",
        "stackfile": "pihole/docker-compose.yml"
      },
      "env": [
        {
          "name": "WEBPASSWORD",
          "label": "Admin Password",
          "description": "Password for admin portal."
        },
        {
          "name": "TZ",
          "label": "Time zone",
          "default": "America/Los_Angeles"
        },
        {
          "name": "TEMPERATUREUNIT",
          "label": "Temperature Unit",
          "select": [
            {
              "text": "Farenheight",
              "value": "f",
              "default": true
            },
            {
              "text": "Celcius",
              "value": "c"
            },
            {
              "text": "Kelvin",
              "value": "k"
            }
          ]
        },
        {
          "name": "VIRTUAL_HOST",
          "label": "Virtual Host",
          "description": "The host name used by Nginx Proxy."
        },
        {
          "name": "UNBOUND_CONFIG_DIR",
          "label": "Unbound config dir",
          "description": "The directory on the server with a-records.conf"
        }
      ]
    },
    {
      "type": 3,
      "title": "Time Machine",
      "description": "Backup target for macOS machines",
      "categories": [
        "backup"
      ],
      "platform": "linux",
      "repository": {
        "url": "https://github.com/brantonb/docker",
        "stackfile": "timemachine/docker-compose.yml"
      },
      "env": [
        {
          "name": "USER",
          "label": "Network share username",
          "default": "timemachine"
        },
        {
          "name": "PASS",
          "label": "Network share password"
        },
        {
          "name": "SHARENAME",
          "label": "Share name",
          "default": "TimeMachine"
        },
        {
          "name": "TMSIZE",
          "label": "Per-machine quota",
          "description": "Size in MB."
        }
      ]
    },
    {
      "type": 3,
      "title": "Wireguard",
      "description": "Fast, modern, secure VPN tunnel",
      "categories": [
        "networking",
        "vpn"
      ],
      "platform": "linux",
      "repository": {
        "url": "https://github.com/brantonb/docker",
        "stackfile": "wireguard/docker-compose.yml"
      },
      "env": [
        {
          "name": "SERVERURL",
          "label": "Server URL"
        },
        {
          "name": "PEERS",
          "label": "Peers",
          "description": "Either a number or a comma-separated list of peer names."
        },
        {
          "name": "LAN_CIDR",
          "label": "LAN CIDR",
          "description": "For split tunneling, the CIDR of your local network."
        },
        {
          "name": "TZ",
          "label": "Time zone",
          "default": "America/Los_Angeles"
        }
      ]
    }
  ]
}
